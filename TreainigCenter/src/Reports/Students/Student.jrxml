	<queryString>
		<![CDATA[SELECT stdyear,stdname,stdphone,EMAIL,case(stdgender)WHEN'0'THEN'Male'ELSE 'Female' end f,
to_char(STDBIRTHDAY,'DD/Mon/YYYY'),
to_number(to_char(Sysdate,'YYYY')) -to_number(to_char(STDBIRTHDAY,'YYYY')) bi,stdimage
FROM STUDENTS
where stdyear between $P{fid} and $P{tid} or
 to_number(to_char(Sysdate,'YYYY')) -to_number(to_char(STDBIRTHDAY,'YYYY')) between $P{FromAge} and  $P{ToAge}
ORDER by STDNAME,STDGENDER]]>
	</queryString>
